<style>
        * { margin: 0; padding: 0; box-sizing: border-box; }

        body {
            max-width: 1240px;
            width: 100%;
            min-height: 100vh;
            background: #080808;
            font-family: 'Cormorant Garamond', serif;
            color: #e8e6e3;
            position: relative;
            margin: 0 auto;
            overflow-x: hidden;
            padding-bottom: 240px;
        }

        .frame {
            position: absolute;
            top: var(--fluid-space);
            left: var(--fluid-space);
            right: var(--fluid-space);
            bottom: var(--fluid-space);
            border: var(--fluid-border-thin) solid #333;
        }

        .corner-tl {
            position: absolute;
            top: var(--fluid-space);
            left: var(--fluid-space);
            width: var(--fluid-corner); height: var(--fluid-corner);
            border-top: var(--fluid-border) solid #d4b896;
            border-left: var(--fluid-border) solid #d4b896;
        }

        .corner-br {
            position: absolute;
            bottom: var(--fluid-space);
            right: var(--fluid-space);
            width: var(--fluid-corner); height: var(--fluid-corner);
            border-bottom: var(--fluid-border) solid #d4b896;
            border-right: var(--fluid-border) solid #d4b896;
        }

        .header {
            position: relative;
            margin: 80px 0 0 80px;
            z-index: 10;
        }

        .section-num {
            font-family: 'IBM Plex Mono', monospace;
            font-size: var(--fluid-tiny);
            color: #d4b896;
            letter-spacing: var(--fluid-tracking-sm);
            margin-bottom: 8px;
        }

        h2 {
            font-size: 38px;
            font-weight: 300;
            letter-spacing: var(--fluid-tracking-md);
            text-transform: uppercase;
            line-height: 1.2;
        }

        .subtitle {
            font-size: var(--fluid-body);
            color: #666;
            font-style: italic;
            margin-top: 10px;
            letter-spacing: 1px;
        }

        .diagram-container {
            position: absolute;
            top: 180px;
            left: 50%;
            transform: translateX(-50%);
        }

        .examples-section {
            position: absolute;
            bottom: 500px;
            left: 80px;
            right: 80px;
        }

        .examples-title {
            font-family: 'IBM Plex Mono', monospace;
            font-size: var(--fluid-eyebrow);
            color: #d4b896;
            letter-spacing: var(--fluid-tracking-sm);
            text-transform: uppercase;
            margin-bottom: 15px;
        }

        .examples-flow {
            display: grid;
            grid-template-columns: repeat(3, 1fr);
            gap: var(--fluid-gap-xs);
        }

        .example-box {
            background: #0a0a0a;
            border: var(--fluid-border-thin) solid #333;
            padding: 18px;
        }

        .example-input {
            font-family: 'IBM Plex Mono', monospace;
            font-size: var(--fluid-tiny);
            color: #d4b896;
            margin-bottom: 10px;
        }

        .example-arrow {
            font-size: var(--fluid-title-sm);
            color: #555;
            margin: 8px 0;
        }

        .example-route {
            font-family: 'IBM Plex Mono', monospace;
            font-size: var(--fluid-tiny);
            color: #888;
            margin-bottom: 8px;
        }

        .example-result {
            font-size: 12px;
            line-height: 1.6;
            color: #888;
        }

        .agent-list {
            position: absolute;
            bottom: 240px;
            left: 80px;
            right: 80px;
            display: grid;
            grid-template-columns: repeat(4, 1fr);
            gap: 12px;
        }

        .agent-card {
            background: #0c0c0c;
            border: var(--fluid-border-thin) solid #2a2a2a;
            padding: 12px;
            position: relative;
        }

        .agent-card::before {
            content: '';
            position: absolute;
            left: 0;
            top: 0;
            bottom: 0;
            width: 2px;
            background: #d4b896;
        }

        .agent-name {
            font-family: 'IBM Plex Mono', monospace;
            font-size: var(--fluid-eyebrow);
            color: #d4b896;
            margin-bottom: 6px;
            letter-spacing: 1px;
        }

        .agent-desc {
            font-size: 11px;
            line-height: 1.5;
            color: #888;
        }

        .nav-bar {
            position: fixed;
            bottom: 0;
            left: 0;
            right: 0;
            height: 60px;
            background: linear-gradient(to bottom, #0a0a0a, #080808);
            border: var(--fluid-border-thin) solid #2a2a2a;
            display: flex;
            align-items: center;
            justify-content: center;
            gap: 4px;
            padding: 0 40px;
            z-index: 1000;
        }

        .nav-item {
            flex: 1;
            height: 32px;
            display: flex;
            align-items: center;
            justify-content: center;
            font-family: 'IBM Plex Mono', monospace;
            font-size: 9px;
            letter-spacing: 1px;
            text-transform: uppercase;
            color: #555;
            background: #0c0c0c;
            border: var(--fluid-border-thin) solid #222;
            transition: all 0.2s;
            cursor: pointer;
            text-decoration: none;
        }

        .nav-item:hover {
            color: #888;
            border-color: #333;
            background: #111;
        }

        .nav-item.active {
            background: #d4b896;
            color: #080808;
            border-color: #d4b896;
            font-weight: 500;
        }

                /* Mobile Hamburger Menu */
        #menu-toggle {
            display: none;
        }

        .hamburger-btn {
            display: none;
            position: fixed;
            top: 20px;
            right: 20px;
            width: 50px;
            height: 50px;
            background: #d4b896;
            border-radius: 4px;
            cursor: pointer;
            z-index: 1001;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            gap: 6px;
            transition: all 0.3s;
        }

        .hamburger-btn span {
            display: block;
            width: 28px;
            height: 3px;
            background: #080808;
            border-radius: 2px;
            transition: all 0.3s;
        }

        .mobile-menu {
            display: none;
            position: fixed;
            top: 0;
            right: -100%;
            width: 300px;
            height: 100vh;
            background: #080808;
            border-left: var(--fluid-border) solid #d4b896;
            z-index: 1000;
            transition: right 0.3s ease;
            overflow-y: auto;
            padding: 80px 20px 20px;
        }

        .mobile-menu-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: var(--fluid-space-sm);
            padding-bottom: 20px;
            border-bottom: 1px solid #333;
        }

        .mobile-menu-title {
            font-family: 'IBM Plex Mono', monospace;
            font-size: var(--fluid-eyebrow);
            color: #d4b896;
            letter-spacing: var(--fluid-tracking-sm);
            text-transform: uppercase;
        }

        .close-btn {
            position: absolute;
            top: 20px;
            right: 20px;
            width: 40px;
            height: 40px;
            background: transparent;
            border: var(--fluid-border) solid #d4b896;
            border-radius: 4px;
            cursor: pointer;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: var(--fluid-title-sm);
            color: #d4b896;
            transition: all 0.2s;
        }

        .close-btn:hover {
            background: #d4b896;
            color: #080808;
        }

        .mobile-nav-item {
            display: block;
            padding: 18px 20px;
            font-family: 'IBM Plex Mono', monospace;
            font-size: var(--fluid-eyebrow);
            letter-spacing: var(--fluid-tracking-sm);
            text-transform: uppercase;
            color: #888;
            background: #0c0c0c;
            border: var(--fluid-border-thin) solid #222;
            border-bottom: none;
            text-decoration: none;
            transition: all 0.2s;
        }

        .mobile-nav-item:last-child {
            border-bottom: 1px solid #222;
        }

        .mobile-nav-item:hover {
            color: #d4b896;
            background: #111;
            border-color: #333;
            padding-left: 30px;
        }

        .mobile-nav-item.active {
            background: #d4b896;
            color: #080808;
            border-color: #d4b896;
            font-weight: 500;
        }

        /* Checkbox hack for menu toggle */
        #menu-toggle:checked ~ .mobile-menu {
            right: 0;
        }

        #menu-toggle:checked ~ .hamburger-btn span:nth-child(1) {
            transform: rotate(45deg) translate(8px, 8px);
        }

        #menu-toggle:checked ~ .hamburger-btn span:nth-child(2) {
            opacity: 0;
        }

        #menu-toggle:checked ~ .hamburger-btn span:nth-child(3) {
            transform: rotate(-45deg) translate(8px, -8px);
        }

        @media (max-width: 768px) {
            .nav-bar {
                display: none;
            }

            .hamburger-btn {
                display: flex;
            }

            .mobile-menu {
                display: block;
            }

            body {
                width: 100%;
                height: auto;
                min-height: 100vh;
                overflow-x: hidden;
                padding-bottom: 140px;
            }

            .content {
                width: 90%;
                padding: var(--fluid-space-xs) 0;
            }

            .title {
                font-size: var(--fluid-title-lg);
                letter-spacing: 10px;
            }

            .stats {
                flex-wrap: wrap;
                gap: var(--fluid-gap);
            }

            .diagram {
                width: 100%;
                height: auto;
            }

            .footer {
                position: fixed;
                bottom: 70px;
                left: 0;
                right: 0;
                flex-wrap: wrap;
                gap: 10px;
            }
        }

        .footer {
            position: fixed;
            bottom: 70px;
            left: 0;
            right: 0;
            display: flex;
            justify-content: space-between;
            font-family: 'IBM Plex Mono', monospace;
            font-size: var(--fluid-eyebrow);
            color: #555;
            letter-spacing: var(--fluid-tracking-sm);
            padding: 0 80px;
            z-index: 999;
        }

        .page-num {
            color: #d4b896;
        }
    </style>
